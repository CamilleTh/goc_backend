GET         /api/transport              controllers.FeelController.transport(lat: Double, lng: Double)
GET         /api/security               controllers.FeelController.security(lat: Double, lng: Double)
GET         /api/health                 controllers.FeelController.health(lat: Double, lng: Double)
GET         /api/digital                controllers.FeelController.digital(lat: Double, lng: Double)
GET         /api/weather                controllers.FeelController.weather(lat: Double, lng: Double)

POST        /api/:category/down         controllers.FeelController.down(category:String)
POST        /api/:category/up           controllers.FeelController.up(category:String)

POST        /api/speeding/:speed        controllers.FeelController.addSpeed(speed:Int)
